{
  "name": "cargo",
  "version": "2.3.0",
  "private": true,
  "engines": {
    "node": "16.x",
    "yarn": "1.22.19"
  },
  "scripts": {
    "heroku-prebuild": "export NPM_CONFIG_PRODUCTION=false; export NODE_ENV=; NPM_CONFIG_PRODUCTION=false NODE_ENV=development yarn install --only=dev --dev --legacy-peer-deps",
    "heroku-postbuild": "export NPM_CONFIG_PRODUCTION=true; export NODE_ENV=production;",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "start": "yarn build && yarn serve:prod",
    "serve": "nuxt",
    "serve:stories": "cross-env MODE=stories nuxt storybook",
    "build": "nuxt build",
    "build:docs": "vue-styleguidist build",
    "serve:prod": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style",
    "create-component": "node index.cli.js component",
    "create-page": "node index.cli.js page",
    "create-store": "node index.cli.js store"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxt/typescript-build": "2.1.0",
    "@nuxtjs/auth-next": "5.0.0-1648802546.c9880dc",
    "@nuxtjs/axios": "5.13.6",
    "@nuxtjs/i18n": "7.2.0",
    "@nuxtjs/proxy": "2.1.0",
    "@nuxtjs/pwa": "3.3.5",
    "core-js": "3.25.5",
    "defu": "5.0.1",
    "dynamsoft-javascript-barcode": "^9.6.11",
    "normalize.css": "8.0.1",
    "nuxt": "2.15.8",
    "nuxt-healthcheck": "1.0.1",
    "v-mask": "2.3.0",
    "vue-barcode-reader": "0.0.3",
    "vue2-touch-events": "3.2.2",
    "@nuxtjs/stylelint-module": "4.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "7.18.9",
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@nuxt/types": "2.15.8",
    "@nuxtjs/eslint-config-typescript": "6.0.0",
    "@nuxtjs/eslint-module": "3.0.2",
    "@nuxtjs/style-resources": "1.0.0",
    "@nuxtjs/svg": "0.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/vue-barcode-reader": "0.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.5",
    "cross-env": "7.0.3",
    "css-loader": "5.2.7",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-nuxt": "2.0.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-vue": "7.7.0",
    "husky": "8.0.1",
    "inquirer": "8.0.0",
    "lint-staged": "13.0.3",
    "nuxt-build-optimisations": "1.0.7",
    "prettier": "2.2.1",
    "sass": "1.55.0",
    "sass-loader": "10.1.1",
    "sass-resources-loader": "2.1.1",
    "stylelint": "13.12.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "21.0.0",
    "stylelint-scss": "3.19.0",
    "vue-loader": "15.9.8"
  }
}
